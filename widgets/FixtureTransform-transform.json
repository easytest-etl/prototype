{
  "metadata": {
    "spec-version": "1.4"
  },
  "display-name": "Fixture (Test)",
  "icon": {
    "type": "inline",
    "arguments": {
      "data": "data:image/png;base64,"
    }
  },
  "configuration-groups": [
    {
      "label": "Fixture Context",
      "properties": [
        {
          "name": "name",
          "label": "Fixture Name",
          "widget-type": "textbox",
          "widget-attributes": {
            "width": "large"
          }
        },
        {
          "name": "fixture-values",
          "label": "Key-value pairs",
          "widget-type": "ds-multiplevalues",
          "widget-attributes": {
            "delimiter": ",",
            "values-delimiter": ":",
            "numValues": "2",
            "placeholders": [
              "Key",
              "Value"
            ]
          }
        }
      ]
    },
    {
      "label": "Assertion Control",
      "properties": [
        {
          "name": "assertion-control",
          "label": "Set state for assertions following this fixture",
          "widget-type": "keyvalue-dropdown",
          "widget-attributes": {
            "delimiter": ",",
            "kv-delimiter": ":",
            "dropdownOptions": [
              "enabled",
              "disabled"
            ]
          }
        }
      ]
    }
  ]
}